<html 
	  xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
	  xmlns:p="tapestry:parameter"> 

	<div>数据导入</div>
	<form t:type="form" t:id="import" t:clientValidation="none" t:autofocus="false">
		<div>
		<t:label for="dayid" />
		<input t:type="textfield" t:id="dayid" value="imInfo.dateId" t:validate="required" label="数据日期" />
		</div>	
		<div>
		 <t:label for="importList" />	
		 <select t:type="select" t:id="importList"  value="imInfo.importTitles" t:model="importTitles" t:validate="required" label="导入列表">
		 </select> 	
		 </div>
		 <div>    
		 	<t:label for="fileName" /> 
		 	<input  t:type="upload" t:id="fileName" value="imInfo.fileName" t:validate="required" label="数据文件" />
		 </div> 
		 <div>
		 	<t:label for="importType" />
		    <t:radiogroup t:id="importType" value="imInfo.importType" label="是否覆盖"> 
			    <t:radio t:id="cover" value="cover" label="是" />
				<t:label for="cover" />
				<t:radio t:id="nocover" value="nocover" label="否" />
			    <t:label for="nocover" />
		    </t:radiogroup>
		 </div> 
 		<input type="submit" value="上传"/>
	</form>
	
